{
    "name": "Quora skip question",
    "version": "1.0",
    "description": "Skip all question in your profile",
    "permissions": ["declarativeContent"],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "content_scripts":[
        {
            "matches": ["https://*.quora.com/answer/requests"],
            "js": ["contentScript.js"],
            "css": ["content.css"]
        }
    ],
    "manifest_version": 2
  }
  